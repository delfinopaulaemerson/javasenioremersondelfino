<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:p="http://primefaces.org/ui">
      
<ui:include src="/template/head.xhtml"/>
<body>
<h:form acceptcharset="ISO-8859-1">
	 <div id="wrapper" class="hasLayout">
			<div id="header">
				<div id="headerinterna">
					<div  id="emape" style="margin-left: 630px;">
						<h1>EMAPE</h1><br />	
					</div>
					<div  id="gestao" style="margin-left: 489px;">
						<h1>Sistema de Validação e Gestão Documental</h1>
					</div>
					<div id="infologin">
						<div id="infologininterna" style="margin-top: 25px;">
							Usu&aacute;rio: <label id="user">#{loginBean.usuarioLogado}</label>&nbsp;
							<h:commandLink action="#{contratanteBean.doSair()}" value="SAIR" immediate="true"/>
							<br />
							<span>Vers&atilde;o 1.0.0</span>
						</div>
					</div>
				</div>
			</div>
		<ui:include src="/template/menu.xhtml"/>
		<div id="cabecalho" style="border-left-width: 100px;margin-top: 50px;margin-left: 700px;">
              <h2>&raquo; <h:commandLink value="Voltar" action="#{contratanteBean.doVoltarCadastoContratante()}" immediate="true" /> &raquo; <label style="color:blue;">Contratante &raquo; Cadastro</label></h2>
				 <div id="mensagem" style="margin-top: 20px;">
   	            	<ul>
   	            			<h2><li><h:messages  showDetail="true" showSummary="false" infoStyle="color:blue;" warnStyle="color: yellow;" errorStyle="color: red;" warnClass="erro"/></li></h2>
   	            	</ul>
   	            </div>	
		</div>
		<br/>
		<br/>
		<div id="fildset">
			<fieldset class="fieldset">
					<legend>Cadastro do Contratante</legend>
						<fieldset class="fieldset">
								<legend>Contratante</legend>
								<div class="grid-6-12">
									<label for="id" class="form-lbl">Razão Social<em class="form-req" title="Obrigatório">*</em></label>
									<h:inputText value="#{contratanteBean.contratante.razaoSocial}" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();"  class="form-txt form-large" required="true" requiredMessage="Razão Social é obrigatório." maxlength="110"  />
								</div>
								<div class="grid-2-12">
									<label for="rg" class="form-lbl">CNPJ<em class="form-req" title="Obrigatório">*</em></label>
									<p:inputMask  mask="99.999.999/9999-99" styleClass="form-txt form-large" value="#{contratanteBean.maskcnpj}" size="20" required="true" requiredMessage="CNPJ é obrigatório" />
								</div>
								<div class="grid-2-12">
									<label for="dtnasc" class="form-lbl">CNAE<em class="form-req" title="Obrigatório">*</em></label>
									<h:inputText id="cnae"  class="form-txt form-large" value="#{contratanteBean.contratante.cnae}" required="true" requiredMessage="CNAE é obrigatório." onkeypress="return onlyDigit(event);" maxlength="20"  title="CNAE" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();"/>
								</div>
								<div class="grid-3-12">
									<label for="mae" class="form-lbl">Optante Pelo Simples Nacional<em class="form-req" title="Obrigatório">*</em></label>
									<h:selectOneRadio  id="optsn" value="#{contratanteBean.opt}"  class="form-txt form-large" required="true" requiredMessage="Optante Pelo Simples Nacional é obrigatório." >
   										<f:selectItem itemValue="Sim" itemLabel="Sim" />
   										<f:selectItem itemValue="Não" itemLabel="Não" />
									</h:selectOneRadio>	
								</div>
								<div class="grid-6-12">
									<label for="id" class="form-lbl">Descrição de Atividade<em class="form-req" title="Obrigatório">*</em></label>
									<h:inputText value="#{contratanteBean.contratante.dscAtividade}" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();"  class="form-txt form-large" rendered="true" maxlength="110" required="true" requiredMessage="Descrição de Atividade é obrigatório." />
								</div>
						</fieldset>
						<fieldset class="fieldset">
								<legend>Enderço do Contratante</legend>
								<div class="grid-7-12">
									<label for="id" class="form-lbl">Rua</label>
									<h:inputText id="rua" value="#{contratanteBean.endereco.rua}" class="form-txt form-large"  maxlength="80"  title="Rua" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();" />
								</div>
								<div class="grid-1-12">
									<label for="rg" class="form-lbl">Numero</label>
									<h:inputText id="numero"   class="form-txt form-large" value="#{contratanteBean.endereco.numero}" onkeypress="return onlyDigit(event);" maxlength="10"  title="Numero" />
								</div>
								<div class="grid-2-12">
									<label class="form-lbl">Complemento</label>
									
									<h:inputText id="compl"   class="form-txt form-large" value="#{contratanteBean.endereco.complemento}" maxlength="50"   title="Complemento" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();"/>
								</div>
								<div class="grid-3-12">
									<label for="nome" class="form-lbl">Cidade</label>
									<h:inputText id="cidade"  class="form-txt form-large" value="#{contratanteBean.endereco.cidade}" maxlength="50" title="Cidade" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();"/>
								</div>
								<div class="grid-4-12">
									<label for="pai" class="form-lbl">Bairro</label>
									<h:inputText id="bairro" class="form-txt form-large" value="#{contratanteBean.endereco.bairro}"  maxlength="50" title="bairro" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();"/>
								</div>
								<div class="grid-2-12">
									<label for="pai" class="form-lbl">Estado</label>
									<h:inputText id="estado" class="form-txt form-large" value="#{contratanteBean.endereco.estado}" maxlength="50" title="estado" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();" />
								</div>
								<div class="grid-2-12">
									<label for="pai" class="form-lbl">Cep</label>
									<h:inputText id="cep" class="form-txt form-large" value="#{contratanteBean.endereco.cep}"  onkeypress="mascara(this,cep)" maxlength="9" title="Cep" />
								</div>
						</fieldset>
						<fieldset class="fieldset">
								<legend>Dados do Responsável</legend>
								<div class="grid-3-12">
									<label for="rg" class="form-lbl">Nome do Gestor da Contratante<em class="form-req" title="Obrigatório">*</em></label>
									<h:inputText id="resp" class="form-txt form-large" value="#{contratanteBean.dados.nome}" required="true" requiredMessage="Nome do Gestor da Contratante é obrigatório." maxlength="50"  title="Nome do Gestor da Contratante" onkeydown="this.value = this.value.toUpperCase();" onkeyup="this.value = this.value.toUpperCase();"/>
								</div>
								<div class="grid-3-12">
									<label for="dtnasc"  class="form-lbl">Telefone<em class="form-req" title="Obrigatório">*</em></label>
									<h:inputText id="tel"  class="form-txt form-large" value="#{contratanteBean.dados.telefone}" required="true" requiredMessage="Telefone é obrigatório." onkeypress="return(maskEvent(this, '(###)####-####',event));"  maxlength="50"  title="Telefone" />
								</div>
								<div class="grid-3-12">
									<label for="dtnasc" class="form-lbl">Email do Gestor da Contratante<em class="form-req" title="Obrigatório">*</em></label>
									<h:inputText id="email" class="form-txt form-large" value="#{contratanteBean.dados.email}" maxlength="50" required="true" requiredMessage="Email do Gestor da Contratante é obrigatório."  title="Email do Gestor da Contratante"/>
								</div>
						</fieldset>				
			</fieldset>	
		<p class="form-req">* Campos obrigatórios </p>	
		</div>
		<div class="botoes" style="margin-top: 25px;margin-bottom: 25px;">
			<h:commandButton type="submit" id="submit" action="#{contratanteBean.cadastrarContratante()}"  class="form-button dir btnPesquisar"  title="Salvar" value="Salvar" />
		</div>
		<div id="rodape" style="margin-top:100px;">
				<p class="nome_sistema">Est. Mar. Miguel Salazar Mendes de Morais, 128, Sala 304, Taquara, RJ, cep 22770-331</p>
		</div>
	</div>
</h:form>			
</body>
</html>