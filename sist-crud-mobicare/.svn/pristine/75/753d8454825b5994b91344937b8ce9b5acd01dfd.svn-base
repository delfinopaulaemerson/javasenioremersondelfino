	  <script type="text/javascript">

	  jQuery(document).ready(function($) {
		  jQuery("#obs_contratoSocial").hide();
		  jQuery("#obs_cartaoCnpj").hide();
		  jQuery("#obs_cndPrevidencia").hide();
		  jQuery("#obs_cndTributaria").hide();
		  jQuery("#obs_crfFgts").hide();
