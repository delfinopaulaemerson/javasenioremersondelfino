/*ESTILOS DE ELEMENTOS DE FORMULRIO*/

/*
classes css da tag input:
- .txt -> text, password, file
- .btn -> submit, button, reset
*/
input, button, select, textarea, legend{font-family:Arial, Helvetica, sans-serif;}
select[multiple="multiple"]{height:63px;}
input.txt, select, textarea{border:1px inset #7f9db9;font-size:13px;font-weight:normal;padding:2px;margin-top:0;}
select{padding:0;margin-top:1px;}
.upper{text-transform:uppercase;}
input.btn{ font-family:Arial,Helvetica; font-weight:bold; font-size:12px;cursor:pointer;}
input.btndisabled{background-color:#FFF;cursor:auto;border:1px solid #BBB;color:#BBB;padding:2px 3px;padding:3px 4px\9;*padding:1px 3px;}
input.btndisabled:hover{background-color:#FFF;cursor:auto;border:1px solid #BBB;color:#BBB;padding:2px 3px;padding:3px 4px\9;*padding:1px 3px;}
input.esq, a.esq{margin-left:10px;float:left;}
input.dir, a.dir{margin-right:10px;float:right;}
input.esqpdir{vertical-align:top;padding:3px 18px;*padding:3px 13px;border:solid 1px #25304a;}
input.dirpesq{vertical-align:bottom;margin-top:19px;padding:3px 18px;*padding:3px 13px;border:solid 1px #25304a;}

a.btn{font-weight:bold;font-size:12px;line-height:24px;}
a.btn:hover{background-color:#dcdcdc;}

a.btnverde{border:1px solid #286A53;background:#589480 url('../imagens/btn_bg.gif') repeat-x;padding:0px 6px;
color:#fff;font-weight:bold;font-size:12px;line-height:22px;}
a.btnverde:hover{border:1px solid #10271F;background:#589480 url('../imagens/btn_bg_h.gif') repeat-x;}

input.txtdisabled, select.txtdisabled,textarea.txtdisabled{background:#fff;color:#000;border:none;overflow: auto;}
input.readonly{border:none;}
input.contador{background:none;border:0;font-size:11px;color:#25304a;}
.displaynone{display:none;}
#form_box label.displaynone{display:none;}

