/*=====================Modal============================*/

#divmodal{margin:0;padding:0;width:100%;height:100%;position:absolute;top:0;left:0;z-index:9999;display:none;}

#divfundomodal{margin:0;padding:0;width:100%;;overflow:hidden;height:9999px;background-color:#c8c8c8;position:fixed;top:0;left:0;z-index:9999;filter:alpha(opacity=90);-ms-filter:alpha(opacity=90);opacity:0.9;display:none;overflow:hidden;}

body:nth-of-type(1) #divfundomodal{position:fixed;height:100%;}/*hack chrome*/

#divModalContent{margin:0 auto;padding:0;width:280px;height:110px;position:absolute;margin-top:15%;z-index:99999;display:none;font-size:1.2em;position:fixed; background:#fff url("../imagens/loadinfo.gif") no-repeat center center; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px;}

#divModalContent fieldset{bottom:0;position:absolute;width:93%;padding:10px;display:none;}

#divModalContent label{margin-right:34px;}

#divModalContent p{font-weight:bold;}

#divModalContent p.subtitulo{padding-top:5px;border:none;}

#divModalContent p.subtitulo label{line-height:20px;width:100%;text-align:center;display:block;}
