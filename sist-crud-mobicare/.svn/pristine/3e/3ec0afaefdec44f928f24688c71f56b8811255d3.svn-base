@charset "UTF-8";

/*GERAIS*/
body{font-size:11px;font-family:Arial,Helvetica,Sans serif;color:#020403;background-color:#fff;}
html, body{margin:0;padding:0;height:100%;}

a{text-decoration:none;border-bottom:1px solid #e7972e;color:#e7972e;}/*color:#f8aa4f;color:#ffaf46;*/
a:hover{color:#e7972e;border-bottom:none;text-decoration:none;}
a:active{position:relative;top:1px;}
a img{vertical-align:middle;cursor:pointer;}
a.disabled img{cursor:default;opacity:0.3;filter:alpha(opacity=30);}
a.disabled:active img{top:0;position: inherit;}
h2{font-size:1.5em;font-weight:bolder;}
h2 span{color:#a2670b;}
h2 a:hover{}
span.obr{color:#FF3300;}
.hidden{display:none;}
.hasLayout{}
textarea{min-height:50px;}
.displayinline{display:inline-block;}
.fontweightnormal{font-weight:normal;}
.red{color:red;}
.nowrap{white-space:nowrap}

.btnjqslide01,.btnjqslide01,.btnjqslide02{cursor:pointer;}
#ifMenu{visibility:hidden;position:absolute;left:-99999px;}

/*JQUERY UI ACERTOS */
#accordion h3 a,.accordion h3 a{border-bottom:none;}
#accordion .accordionpaginacao,.accordion .accordionpaginacao{background:#fff url(../imagens/grid_shadow.jpg) bottom repeat-x;color:#666;font-size:10px;overflow:hidden;padding:10px 3px;margin:-1.2em -2.6em;bottom:0;*overflow:inherit;text-align:right;}
#accordion .accordionpaginacao a,.accordion .accordionpaginacao a{background:#fff;border:1px solid #ccc;border-color:#ccc #aaa #aaa #ccc;color:#666;margin:0 3px;padding:2px 6px;}
#accordion .accordionpaginacao a:hover,.accordion .accordionpaginacao a:hover{border:1px solid #ccc;border-color:#ccc #aaa #aaa #ccc;}
#accordion .accordionpaginacao span,.accordion .accordionpaginacao span{padding:2px 3px;}
#abas ul.ui-tabs-nav li a{border-bottom:none;font-weight:bold;}
.ui-autocomplete a{border:none;}
.ui-autocomplete-input{width:99.5%;*width:100%;border:1px inset #7F9DB9;}
.ui-datepicker-trigger {cursor: pointer;display: block;float: right;margin-top: -17px;*margin-top: -19px;position: relative;vertical-align: top;
position:absolute\0/IE8+9; /* hack ie8 e ie9 */
margin-top:-17px\0/IE8+9; /* hack ie8 e ie9 */
right:5px\0/IE8+9; /* hack ie8 e ie9 */
}
@media screen and (-webkit-min-device-pixel-ratio:0){ /* hack chrome */
.ui-datepicker-trigger {margin-top: -19px;} 
}

