/*=====================Modal============================*/

#divmodal{margin:0;padding:0;width:100%;height:100%;position:absolute;top:0;left:0;z-index:9999;display:none;}

#divmodal iframe.ifmodal{width: 100%; height: 100%; position: absolute; top: 0pt; left: 0pt;overflow:hidden;z-index:1;}

#divfundomodal{margin:0;padding:0;width:100%;;overflow:hidden;height:100%;background-color:#c8c8c8;position:fixed;top:0;left:0;z-index:9999;filter:alpha(opacity=80);display:none;background:url("../imagens/bg_modal.png") repeat left top;}/*-ms-filter:alpha(opacity=80);opacity:0.8;*/

body:nth-of-type(1) #divfundomodal{position:fixed;height:100%;}/*hack chrome*/

#divModalContent,#divModalContent2{margin:-75px 0 0 -140px;padding:0;width:280px;height:110px;position:absolute;top:50%;left:50%;z-index:99999;display:none;font-size:1.2em;position:fixed; background:#fff url("../imagens/loadinfo.gif") no-repeat center center; -moz-border-radius: 4px; -webkit-border-radius: 4px; border-radius: 4px;}

#divModalContent2{width:80%;height:80%;margin:auto;top:10%;left:10%;overflow:auto;background:none;}

#divModalContent fieldset{bottom:0;position:absolute;width:93%;padding:10px;display:none;}

#divModalContent2 fieldset{padding:10px;background-color:#fff;}

#divModalContent2 fieldset legend{font-size: 0.8em;}

#divModalContent2 fieldset.fsmodal fieldset div label{font-size:0.9em;}

#divModalContent2 fieldset.fsmodal fieldset p{font-size:0.9em;}

#divModalContent label{margin-right:34px;}

#divModalContent p{font-weight:bold;}

#divModalContent p.subtitulo{padding-top:5px;border:none;}

#divModalContent p.subtitulo label{line-height:20px;width:100%;text-align:center;display:block;}
